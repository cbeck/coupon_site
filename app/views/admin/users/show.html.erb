<p><b>Email:</b>  <%= @user.email %></p>
<% unless @user.location.nil? -%>
	<p><b>Address:</b> <%= @user.location.to_html %></p>
<% end -%>
<% unless @user.email2.nil? -%>
	<p><b>Email 2:</b> <%= @user.email2 %></p>
<% end -%>
<% unless @user.email3.nil? -%>
	<p><b>Email 3:</b> <%= @user.email3 %></p>
<% end -%>
<p><b>Member since:</b> <%= time_ago_in_words(@user.created_at) %> ago.</p>

<%= link_to 'Edit', edit_admin_user_path(@user) %> |
<%= link_to 'Back', admin_users_path %>
